{
  "name": "@vir3lsa/gonorth",
  "version": "0.1.4",
  "description": "A JavaScript interactive fiction engine",
  "main": "./lib/gonorth.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --ignore '**/*.test.js' --source-maps 'inline'",
    "build:whiteroom": "webpack --env UI_TEST_MODE --config 'examples/white_room/webpack.config.js'",
    "build:cypress": "npm run build && npm run build:whiteroom",
    "test": "jest",
    "test:cypress": "cypress open",
    "coverage": "jest --coverage",
    "start:bridges": "webpack-dev-server --config 'examples/bridges/webpack.config.web.js'",
    "start:whiteroom": "http-server examples/white_room/dist/"
  },
  "keywords": [],
  "author": "Rich Locke",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/richlocke87/gonorth.git"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "babel-jest": "25.1.0",
    "css-loader": "6.7.1",
    "cypress": "10.1.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.1",
    "jest": "27.2.0",
    "jest-extended": "0.11.5",
    "style-loader": "3.3.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/polyfill": "7.6.0",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.2",
    "aws-sdk": "2.1209.0",
    "debounce": "1.2.1",
    "file-loader": "6.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "6.0.3",
    "react-redux": "8.0.2",
    "react-scroll": "1.8.7",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "remark-gfm": "1.0.0",
    "seedrandom": "3.0.5",
    "uuid": "8.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testPathIgnorePatterns": [
      "cypress"
    ]
  }
}

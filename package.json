{
  "name": "gonorth",
  "version": "0.0.1",
  "description": "",
  "main": "./lib/gonorth.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore '**/*.test.js' --source-maps 'inline'",
    "test": "jest",
    "test:cypress": "cypress open",
    "coverage": "jest --coverage",
    "start:grotto": "webpack-dev-server --config 'examples/witchs_grotto/webpack.config.web.js'",
    "start:grotto-server": "webpack --config 'examples/witchs_grotto/webpack.config.server.js' && node examples/witchs_grotto/dist-server/main.js",
    "start:bridges": "webpack-dev-server --config 'examples/bridges/webpack.config.web.js'",
    "start:bridges-server": "webpack --config 'examples/bridges/webpack.config.server.js' && node examples/bridges/dist-server/main.js",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": "Rich Locke",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/richlocke87/gonorth.git"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "25.1.0",
    "cypress": "^9.5.3",
    "file-loader": "6.2.0",
    "jest": "27.2.0",
    "jest-extended": "0.11.5",
    "prompts": "2.2.1",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/polyfill": "7.6.0",
    "@material-ui/core": "4.4.2",
    "@material-ui/icons": "4.4.1",
    "debounce": "1.2.1",
    "patch-package": "6.4.7",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-markdown": "4.2.2",
    "react-redux": "7.1.1",
    "react-scroll": "1.8.3",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testPathIgnorePatterns": [
      "cypress"
    ]
  }
}
